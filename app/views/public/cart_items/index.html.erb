<div class="contanier">
  <div class="row">
    <div class="col-10">
      <% if @cart_items.present? %>
        <% @cart_items.each do |cart_item| %>
          <%= cart_item.item.name %>
          <%= cart_item.quantity %>
        <% end %>
      <% else %>
        <p>商品がありません。</p>
      <% end %>
    </div>
  </div>
</div>